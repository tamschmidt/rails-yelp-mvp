<%= form_for(@restaurant) do |f|%>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :address %>
  <%= f.text_field :address %>
  <%= f.label :phone_number%>
  <%= f.text_field :phone_number%>
  <%= f.label :category%>
  <%= f.text_field :category%>
  <%= f.submit %>
<% end %>

<i class="fas fa-pen"></i>
<%= link_to 'Back to list', restaurants_path %>
